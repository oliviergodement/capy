<!DOCTYPE html>
<html>
  <head>
    <title>Capy</title>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true, :media => 'all' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'layouts/navbar' %>
    <div class="container">
      <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>">
            <ul>
              <li>
                <%= value %>
              </li>
            </ul>
        </div>
      <% end %>
      <%= yield %>
    </div>
  </body>
</html>